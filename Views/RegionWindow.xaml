<Window x:Class="EU4ModUtil.Views.RegionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EU4ModUtil.Views"
        xmlns:object="clr-namespace:EU4ModUtil.Models.Data.Generic"
        mc:Ignorable="d"
        Title="Regions" Height="450" Width="800">
    <Grid>
        <Grid.DataContext>
            <object:TreeItem/>
        </Grid.DataContext>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="4*" />
            <ColumnDefinition Width="2*" />
        </Grid.ColumnDefinitions>
        <Label Name ="attributeName" Grid.Column="1" Content="Unknown" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button Grid.Column="1" Content=" Add " HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <Button Grid.Column="1" Content=" Delete " HorizontalAlignment="Left" Margin="48,41,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
